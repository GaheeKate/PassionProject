@model PassionProject.Models.OrderDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this order?</h2>
<a href="/Order/List">Cancel</a>
<h2>Order Id: @Model.Id</h2>

<form action="/Order/Delete/@Model.Id" method="POST">
    <input type="submit" value="Confirm Delete" />

</form>


